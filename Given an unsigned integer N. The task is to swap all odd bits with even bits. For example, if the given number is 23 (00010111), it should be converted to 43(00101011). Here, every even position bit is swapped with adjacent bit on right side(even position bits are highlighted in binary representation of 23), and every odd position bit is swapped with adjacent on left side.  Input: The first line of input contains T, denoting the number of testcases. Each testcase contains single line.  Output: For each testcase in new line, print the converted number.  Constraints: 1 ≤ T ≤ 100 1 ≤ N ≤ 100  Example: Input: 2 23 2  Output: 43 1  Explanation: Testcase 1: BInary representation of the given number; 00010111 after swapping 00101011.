#include <bits/stdc++.h>
using namespace std;

int main() {
	int t; cin>>t; while(t--){
	    int n; cin>>n;
	    int temp[8]={0},c=0,count[8]={0};
	    while(n>0){
	        temp[c]= n&1;
	        c++;
	        n>>=1;
	    }
	   // cout<<"c="<<c<<endl;
	    int k=8-c;
	   // c=0;
	    for(int i=k-1; i<8; i++){
	        count[i]=temp[c--];
	    }
	   //  for(int i=0; i<8; i++) cout<<temp[i];
	    //cout<<'\n';
	    //for(int i=0; i<8; i++) cout<<count[i];
	    //cout<<'\n';
	    for(int i=0; i<8; i=i+2){
	        int t=count[i];
	        count[i]=count[i+1];
	        count[i+1]=t;
	    }
	    long long int ans=0,pos=1;
	    for(int i=7; i>=0; i--){
	        ans+= count[i]*pos;
	        pos<<=1;
	    }
	    cout<<ans<<endl;
	}	    
	return 0;
}

