#include <bits/stdc++.h>
using namespace std;

int mostfreq(int a[], int n)
{
    unordered_map<int,int> freq;
    for(int i=1;i<=n;i++)
    {
        freq[a[i]]++;
    }
    int max=0,res=-1;
    for(auto c: freq)
    {
        if(c.second > max) 
         {
             max=c.second;
             res=c.first;
         }
    }
return res;
}

int main()
{
    int n=5; 
   // cin>>n;
    int a[n] = {2,2,2,1,3};
    //for(int i=1;i<=n;i++) cin>>a[i];
    cout<<mostfreq(a,n);
}
