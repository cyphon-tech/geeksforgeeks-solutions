using namespace std;

int main() {
	int t; cin>>t; while(t--)
	{
	    int n,m,num;
	    cin>>n>>m;
	    vector<int> v;
	    for(int i=0;i<n;i++) 
	     {
	         cin>>num;
	         v.push_back(num);
	     }
	     for(int i=0;i<m;i++) 
	     {
	         cin>>num;
	         v.push_back(num);
	     }
	     unordered_map<int,int> freq;
	     for(int i=0;i<v.size();i++)
	       freq[v[i]]++;
	  cout<<freq.size()<<endl;
}

	return 0;
}
